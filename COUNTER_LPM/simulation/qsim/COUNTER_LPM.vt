`timescale 1 ps/ 1 ps
module COUNTER_LPM_vlg_sample_tst(
	CLC,
	CLS,
	SET,
	sampler_tx
);
input  CLC;
input  CLS;
input [4:0] SET;
output sampler_tx;

reg sample;
time current_time;
always @(CLC or CLS or SET)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

